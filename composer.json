{
    "name": "srlabs/parley",
    "description": "Polymorphic Messaging for Laravel",
    "keywords": ["laravel", "notifications", "messaging", "polymorphic", "polymorphism"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan C. Durham",
            "email": "ryan@stagerightlabs.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/database": "4.2.*",
        "illuminate/events": "4.2.*",
        "mitch/hashids": "1.*"
    },
    "require-dev": {
        "orchestra/testbench": "2.2.*",
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.2"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "tests"
        ],
        "psr-0": {
            "Parley": "src/"
        }
    },
    "minimum-stability": "stable"
}
